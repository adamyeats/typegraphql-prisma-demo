{
  "name": "typegraphql-prisma-demo",
  "version": "1.0.0",
  "description": "Created using `create-dtnl-nodejs`, seed repo for DTNL Node.js projects",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -p tsconfig.json -w --preserveWatchOutput",
    "dev": "concurrently \"npm:start:debug\" \"npm:build:watch\"",
    "prepare": "husky install",
    "start": "node dist/index.js",
    "start:debug": "NODE_ENV=development DEBUG=dept-node:* nodemon dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "author": "Dept",
  "dependencies": {
    "@prisma/client": "^3.4.2",
    "apollo-server": "^3.6.2",
    "class-validator": "^0.13.2",
    "dotenv": "^16.0.0",
    "graphql": "^16.3.0",
    "graphql-fields": "^2.0.3",
    "graphql-scalars": "^1.14.1",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1"
  },
  "devDependencies": {
    "@types/graphql-fields": "^1.3.4",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.15",
    "concurrently": "^7.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.8.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-security": "^1.4.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.3",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "prisma": "^3.4.2",
    "ts-jest": "^27.1.3",
    "typegraphql-prisma": "^0.18.4",
    "typescript": "^4.5.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{ts,js}": "prettier --write"
  },
  "volta": {
    "node": "16.13.0",
    "npm": "8.1.3"
  }
}
