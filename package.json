{
  "name": "typegraphql-prisma-demo",
  "version": "1.0.0",
  "description": "Created using `create-dtnl-nodejs`, seed repo for DTNL Node.js projects",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -p tsconfig.json -w --preserveWatchOutput",
    "dev": "concurrently \"npm:start:debug\" \"npm:build:watch\"",
    "prepare": "husky install",
    "start": "node dist/index.js",
    "start:debug": "NODE_ENV=development DEBUG=dept-node:* nodemon dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "author": "Dept",
  "dependencies": {
    "@prisma/client": "^4.16.2",
    "apollo-server": "^3.13.0",
    "class-validator": "^0.14.1",
    "dotenv": "^16.4.5",
    "graphql": "^16.8.1",
    "graphql-fields": "^2.0.3",
    "graphql-scalars": "^1.22.4",
    "reflect-metadata": "^0.2.1",
    "type-graphql": "^1.1.1"
  },
  "devDependencies": {
    "@types/graphql-fields": "^1.3.9",
    "@types/jest": "^27.5.0",
    "@types/node": "^20.12.8",
    "concurrently": "^8.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-security": "^3.0.0",
    "husky": "^9.0.11",
    "jest": "^27.5.1",
    "lint-staged": "^15.2.2",
    "nodemon": "^3.1.4",
    "prettier": "^3.2.5",
    "prisma": "^4.16.2",
    "ts-jest": "^27.1.4",
    "typegraphql-prisma": "^0.26.0",
    "typescript": "^4.9.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{ts,js}": "prettier --write"
  },
  "volta": {
    "node": "16.13.0",
    "npm": "8.1.3"
  }
}
