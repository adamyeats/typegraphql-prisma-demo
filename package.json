{
  "name": "typegraphql-prisma-demo",
  "version": "1.0.0",
  "description": "Created using `create-dtnl-nodejs`, seed repo for DTNL Node.js projects",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -p tsconfig.json -w --preserveWatchOutput",
    "dev": "concurrently \"npm:start:debug\" \"npm:build:watch\"",
    "prepare": "husky install",
    "start": "node dist/index.js",
    "start:debug": "NODE_ENV=development DEBUG=dept-node:* nodemon dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "author": "Dept",
  "dependencies": {
    "@prisma/client": "^2.26.0",
    "apollo-server": "^3.1.2",
    "class-validator": "^0.13.1",
    "dotenv": "^10.0.0",
    "graphql": "^15.5.1",
    "graphql-fields": "^2.0.3",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1"
  },
  "devDependencies": {
    "@types/graphql-fields": "^1.3.3",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.4.13",
    "concurrently": "^6.2.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-security": "^1.4.0",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "lint-staged": "^11.1.2",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "prisma": "^2.26.0",
    "ts-jest": "^27.0.4",
    "typegraphql-prisma": "^0.14.8",
    "typescript": "^4.3.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{ts,js}": "prettier --write"
  },
  "volta": {
    "node": "14.17.1",
    "npm": "7.18.1"
  }
}
